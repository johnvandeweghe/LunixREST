{
    "name": "johnvandeweghe/lunixrest",
    "description": "A lightweight REST server",
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "authors": [
        {
            "name": "John VanDeWeghe",
            "email": "vandeweghe@lunixlabs.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "LunixREST\\": "src/"
        }
    },
    "suggest": {
        "doctrine/doctrine": "Required for implementations that use DoctrineEndpointFactory and DoctrineEndpoint"
    }
}
